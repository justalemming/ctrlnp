{
  "manifest_version": 3,
  "name": "Ecma Ctrl+P/N for dropdowns",
  "version": "1.0",
  "description": "Use Ctrl+N and Ctrl+P to navigate up and down in dropdowns",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": ["<all_urls>"] 
    }
  ]
}
