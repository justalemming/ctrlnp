{
  "manifest_version": 3,
  "name": "Ctrl+N/P unix navigation for dropdowns",
  "version": "1.1",
  "description": "Use Ctrl+N and Ctrl+P to navigate up and down in dropdowns",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": ["<all_urls>"] 
    }
  ]
}
